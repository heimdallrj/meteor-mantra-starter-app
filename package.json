{
  "name": "mantra-demo",
  "version": "0.4.0",
  "scripts": {
    "lint": "eslint ./lib ./client ./server ./app --ext .js",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha .scripts/mocha_boot.js app/**/tests/**/*.js --compilers js:babel-core/register",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook -p 9001 -s ./public",
    "dev": "meteor -p 5005 --settings .scripts/dev_settings.json"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.12.0",
    "babel-core": "6.x.x",
    "babel-plugin-react-require": "2.x.x",
    "babel-polyfill": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-react": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "babel-root-slash-import": "1.x.x",
    "chai": "3.x.x",
    "enzyme": "^2.2.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.2.3",
    "jsdom": "^8.0.4",
    "mocha": "2.x.x",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^0.14.8",
    "sinon": "1.17.x",
    "style-loader": "^0.13.1",
    "ud": "^3.0.1"
  },
  "dependencies": {
    "domready": "^1.0.8",
    "json-loader": "^0.5.4",
    "mantra-core": "^1.5.0",
    "markdown-to-react-components": "^0.2.0",
    "material-ui": "^0.14.4",
    "meteor-node-stubs": "^0.2.3",
    "radium": "^0.17.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-mounter": "^1.2.0",
    "react-tap-event-plugin": "^0.2.2",
    "reflexbox": "^1.1.2"
  },
  "private": true
}
